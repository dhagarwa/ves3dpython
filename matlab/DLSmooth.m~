function val = DLSmooth(y, S)
    val = 0;
    for ii=1:length(S.patches)
       patch = S.patches(ii);
       q = fooVec()
       val = val + DLSmoothPatch(y, patch, q);
        
        
    end
    
    
    %Hard coded for on surface singularity correction
    R = 1;
    theta = pi/2;
    phi = pi/3;
    val = val + 0.5*fooVec([R*sin(theta)*cos(phi) R*sin(theta)*sin(phi) R*cos(theta)]);






end
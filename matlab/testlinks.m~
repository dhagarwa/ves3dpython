function [r1, r2] = testlinks(S)
    patches = S.patches;
    p1 = patches(1);
    p2 = patches(5);
    
    for ii=1:size(p1.r, 1)
       
        r0 = p1.r(ii, :);
        patchParameterise(r0, p1, p2);
        
        
    end
    
    u2 = patch.links(:, 7);
    v2 = patch.links(:, 8);
    r2 = sph2cartPatch(u2, v2, patches(4));
    r1 = patch.r;
    

end